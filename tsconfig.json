{
  "compilerOptions": {
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "target": "ES2020",
    "sourceMap": true,
    "outDir": "dist",
    "forceConsistentCasingInFileNames": true, //Optional
    "esModuleInterop": true, //Optional
    "strict": true, //Optional
    "skipLibCheck": true, //Optional
    "alwaysStrict": true
    //"declaration": true
  },
  "include": ["src/**/*"]
}
